\mode<presentation>

% Bildene deklareres bare her, da blir de kun fysisk inkludert en
% gang i hver pdf-fil som genereres, selv om de brukes på hver side.

% Logo nede til høyre på hver side:
\pgfdeclareimage[height=7mm,interpolate=true]{skapendeunivlite}{gfx/english-logo}

% Bakgrunnsbilde for forsida:
\pgfdeclareimage[height=\paperheight,interpolate=true]{bakgrunnforside}{gfx/backGroundFirstPage}

\usetheme{polimi}

\mode
<all>

